.container-panel {
  font-size 12px;
  line-height 20px;

  &-title {
    font-size: 16px;
    font-weight 700;
    margin-bottom 16px;
  }

  &-little-title {
    font-size 14px;
    margin-bottom 8px;
    font-weight 600;
  }

  &-notes {
    background #f8f8f8;
    margin-bottom 8px;
    border: 1px solid #ddd;
    -webkit-border-radius: 4px;
    -moz-border-radius: 4px;
    border-radius: 4px;
    padding: 4px 8px;
  }
}
