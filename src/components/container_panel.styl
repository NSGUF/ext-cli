.container-panel {
  font-size 12px;
  line-height 20px;

  &-title {
    font-size: 16px;
    font-weight 700;
    margin-bottom 32px;
  }

  &-little-title {
    font-size 14px;
    margin-bottom 8px;
    font-weight 600;
  }

  &-notes {
    background #f8f8f8;
    margin-bottom 8px;
    border: 1px solid #ddd;
    -webkit-border-radius: 4px;
    -moz-border-radius: 4px;
    border-radius: 4px;
    padding: 4px 8px;
  }

  &-box {
    padding: 4px 0;
    border:1px solid #ddd;
    -webkit-border-radius: 4px;
    -moz-border-radius: 4px;
    border-radius: 4px;
    margin-right 8px;

    .source-title {
      border-color: #ddd;
      border-radius: 4px;
      box-shadow: 0 1px 1px rgba(0,0,0,.05)
      margin-bottom: 8px;

      &-name {
        font-size: 14px;
        margin-right 32px;
      }


    }

  }

  .run-btn {
    background
  }
}
